config_version=5

[WAT]

Test_Directory="res://tests"
Results_Directory="res://testResults"
Test_Metadata_Directory="res://testMetadata"
Tags=PackedStringArray("Authentication", "RealtimeDB")
Cache_Tests=false
Window_Size=Vector2(1280, 720)
Minimize_Window_When_Running_Tests=false
Port=6008
Display=8

[application]

config/name="SharpGodotFirebaseTest"
run/main_scene="res://World.tscn"
config/features=PackedStringArray("4.0", "C#")
config/icon="res://icon.png"

[dotnet]

project/assembly_name="SharpGodotFirebaseTest"

[editor_plugins]

enabled=PackedStringArray("res://addons/WAT/plugin.cfg")

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/defaults/default_environment="res://default_env.tres"
